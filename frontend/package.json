{
  "name": "web-react-app-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --src ./src --schema ./src/schema.graphql",
    "relay:watch": "relay-compiler --watch --src ./src --schema ./src/schema.graphql",
    "deploy": "aws s3 sync ./build s3://boucekm-web-react-app --acl public-read --delete --only-show-errors"
  },
  "dependencies": {
    "foundation-sites": "^6.5.1",
    "node-sass": "^4.10.0",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-relay": "^1.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "babel-plugin-relay": "^1.7.0",
    "graphql": "^14.0.2",
    "react-test-renderer": "^16.6.3",
    "relay-compiler": "^1.7.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
